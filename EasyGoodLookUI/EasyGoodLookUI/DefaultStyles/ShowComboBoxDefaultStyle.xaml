<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:EasyGoodLookUI">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/EasyGoodLookUI;component/Themes/ShowComboBox.xaml"></ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>
    <Style TargetType="local:ShowComboBox" BasedOn="{StaticResource ShowComboBoxBase}">
        <Setter Property="NameStr" Value="name"></Setter>
        <Setter Property="BorderColor" Value="Blue"></Setter>
        <Setter Property="ItemArray" Value="1,2,3,4,5"></Setter>
    </Style>
</ResourceDictionary>