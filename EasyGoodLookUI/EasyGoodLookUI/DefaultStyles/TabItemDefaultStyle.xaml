<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:EasyGoodLookUI">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/EasyGoodLookUI;component/Themes/TabItem.xaml"></ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>
    <Style TargetType="TabItem" BasedOn="{StaticResource TabItemStyleBase}">
        <Setter Property="BorderThickness" Value="0,0,0,0"></Setter>
        <Setter Property="Width" Value="70"></Setter>
        <Setter Property="Height" Value="50"></Setter>
    </Style>
</ResourceDictionary>