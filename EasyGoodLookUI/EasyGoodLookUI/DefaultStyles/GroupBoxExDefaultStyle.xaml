<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:EasyGoodLookUI">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/EasyGoodLookUI;component/Themes/GroupBoxEx.xaml"></ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>
    <Style TargetType="local:GroupBoxEx" BasedOn="{StaticResource GroupBoxExBase}">
        <Setter Property="Header" Value="GroupBox"></Setter>
        <Setter Property="Background" Value="LightSlateGray"></Setter>
    </Style>
</ResourceDictionary>